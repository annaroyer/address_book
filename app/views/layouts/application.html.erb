<!DOCTYPE html>
<html>
  <head>
    <title>AddressBook</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= form_tag('search/companies', method: 'get') do %>
      <%= label_tag(:q, "Search Companies by Name") %>
      <%= text_field_tag(:q) %>
      <%= submit_tag('Search') %>
    <% end %>
    <%= yield %>
  </body>
</html>
